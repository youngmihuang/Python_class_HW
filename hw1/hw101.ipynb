    "# coding=utf-8\n",
    "'''\n",
    "計算 10!\n",
    "Author: Youngmi\n",
    "'''\n",
    "def factorial(x):\n",
    "    if x == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return factorial(x-1)*x\n",
    "\n",
    "print factorial(10)"
